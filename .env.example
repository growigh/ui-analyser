# MongoDB root credentials used by the mongo container
MONGO_INITDB_ROOT_USERNAME=
MONGO_INITDB_ROOT_PASSWORD=

# API service environment
# Connection string should not be hardcoded in compose. Use env here.
MONGODB_URI=
REDIS_HOST=redis
REDIS_PORT=6379
ANALYZER_API_URL=http://agent:8000
API_PORT=3001
NODE_ENV=development
CLIENT_URL=
CLIENT_URLS= # comma-separated list, e.g., https://uianalyzer.in,https://www.uianalyzer.in

# Optional: external ports
# HOST_MONGO_PORT=27018
# HOST_REDIS_PORT=6380

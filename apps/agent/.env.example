# UI Analyzer API Configuration

# API key for authentication
# Required for production, set a strong, unique value
API_KEY=your-secure-api-key-here

# Port to run the API server on (default: 8000)
PORT=8000

# OpenAI API Key for the AI analyzer
OPENAI_API_KEY=your-openai-api-key

# Google AI API Key (if using Google's AI)
GOOGLE_API_KEY=your-google-api-key

# AI Model configurations
AI_MODEL=gpt-4-vision-preview
AI_TEMPERATURE=0.2
AI_MAX_TOKENS=1024

# Browser configurations
MAX_BROWSERS=4
MAX_TABS_PER_BROWSER=8
SCREENSHOT_TIMEOUT=30000
AUTO_START_DOCKER=true
